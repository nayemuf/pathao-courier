{
    "name": "nayemuf/pathao-courier",
    "description": "Laravel package for Pathao Courier Merchant API integration with caching and rate limiting",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nayem Uddin",
            "email": "nayem110899@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/cache": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Nayemuf\\PathaoCourier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nayemuf\\PathaoCourier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nayemuf\\PathaoCourier\\PathaoCourierServiceProvider"
            ],
            "aliases": {
                "PathaoCourier": "Nayemuf\\PathaoCourier\\Facades\\PathaoCourier"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
